        <activity
            android:name="Korean_Activity"
            android:label="@layout/korean_activity" >
        </activity>
        <activity
            android:name="Usa_Activity"
            android:label="@layout/usa_activity" >
        </activity>
        <activity
            android:name="Korean_Table_Status"
            android:label="@layout/table_state" >
